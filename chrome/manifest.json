{
  "name" :"WebMark",
  "version" :"0.1",
  "manifest_version": 2,
  "description" :"next gen bookmark",
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action" :
  {
    "default_popup" :"index.html",
    "default_icon" : "media/cube.png"
  }
}
